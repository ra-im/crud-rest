
<!--include header--> 
<%- include('include/_header') %> 
<!--/include header-->

<!--main -->

<main id="site-main">
    <div class="container">
        <div class="box-nav.d-flex-justify-between">
            <a href="/add-user" class="border-shadow">
                <span class="text-gradient">Add User <i class="fa fa-user-plus" aria-hidden="true"></i></span>
            </a>
        </div>

        <!--form handle-->

        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>@email</th>
                        <th>Gender</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>

                <!--include table data-->
                    <%- include('include/_tabledata') %>
                <!--/include table data-->

                </tbody>
            </table>
        </form>
        <!--/form handle-->
    </div>
</main>

<!--/main -->

<!--include footer--> 
<%- include('include/_footer') %> 
<!--/include footer-->

